flyway.url=jdbc:postgresql://${POSTGRES_HOST}:5432/${POSTGRES_DB}
flyway.user=${POSTGRES_USER}
flyway.password=${POSTGRES_PASSWORD}
flyway.baselineOnMigrate=false
flyway.locations=filesystem:/flyway/sql/main,filesystem:/flyway/sql/dev-seed
flyway.placeholders.defaultUserEmail=${DEFAULT_USER_EMAIL}
flyway.placeholders.defaultUserPhotoUrl=${DEFAULT_USER_PHOTO_URL}
flyway.placeholders.defaultUserExternalId=${DEFAULT_USER_EXTERNAL_ID}