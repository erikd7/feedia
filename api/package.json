{
  "name": "feedia-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "ts-node ./bin/www.ts",
    "dev": "nodemon",
    "lint": "eslint .",
    "build-ts": "tsc",
    "build": "webpack",
    "test": "jest --watch",
    "ci-test": "jest --coverage",
    "serve": "pm2 start -f build/index.js --name feedia-api --watch --ignore-watch='node_modules keys/*'",
    "deploy-setup": "npm install && npm run build && pm2 reload all",
    "deploy": "npm install && npm run build && npm run serve",
    "deploy-down": "pm2 stop feedia-api",
    "docker-serve": "node build/index.js"
  },
  "dependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/node": "^7.18.5",
    "@hapi/joi": "^17.1.1",
    "@types/debug": "^4.1.8",
    "@types/express-session": "^1.17.7",
    "@types/http-status-codes": "^1.2.0",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/lodash": "^4.14.198",
    "@types/morgan": "^1.9.5",
    "@types/passport": "^1.0.12",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.9",
    "@types/pg": "^8.10.2",
    "@types/yesql": "^4.1.1",
    "babel-plugin-root-import": "^6.6.0",
    "body-parser": "^1.20.0",
    "debug": "~2.6.9",
    "dotenv": "^16.0.1",
    "es6-promise": "^4.2.8",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "http-status-codes": "^2.2.0",
    "joi": "^17.6.0",
    "joi-oid": "^1.1.2",
    "jsonwebtoken": "^9.0.2",
    "jwt-passport": "^0.0.5",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.7.3",
    "pm2": "^5.2.0",
    "swagger-ui-express": "^4.5.0",
    "yamljs": "^0.3.0",
    "yesql": "^5.0.0"
  },
  "babel": {
    "retainLines": true,
    "presets": [
      [
        "@babel/preset-env"
      ]
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@types/express": "^4.17.17",
    "babel-loader": "^8.2.5",
    "eslint": "^8.19.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "nodemon": "^2.0.18",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  }
}
